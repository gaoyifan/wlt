table inet wlt {
    map src2mark {
        type ipv4_addr : mark
        flags interval, timeout
        elements = {
            1.1.1.1/32 timeout 1m: 0x0001
        }
    }

    chain prerouting {
        type filter hook prerouting priority mangle - 1; policy accept;
        meta mark set ip saddr map @src2mark
    }
}